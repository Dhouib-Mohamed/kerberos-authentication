
; BIND reverse data file for local loopback interface
;
$TTL    604800
@ IN SOA ns1.gl4.tn. root.ns1.gl4.tn. (
                     3013040200         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      ns1.gl4.tn.
100     IN      PTR     gl4.tn.
101     IN      PTR     otherdomain.com.
